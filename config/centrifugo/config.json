{
   "client": {
      "allowed_origins": [
         "*"
      ],
      "proxy": {
         "connect": {
            "enabled": true,
            "endpoint": "http://host.docker.internal:6080/connect"
         }
      }
   },
   "http_api": {
      "key": "my_api_key"
   },
   "admin": {
      "password": "password",
      "secret": "secret",
      "enabled": true
   },
   "channel": {
      "without_namespace": {
         "allow_subscribe_for_client": true,
         "allow_subscribe_for_anonymous": true
      }
   },
   "log": {
      "level": "trace"
   }
}